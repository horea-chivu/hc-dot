# Git aliases
alias gi='git init'
alias gaa='git add .'
alias gap='git add -p'
alias gpull='git pull'
alias gpom='git push origin master'
alias gs='git status'
alias gl='git log'
alias gma='git merge --abort'
alias gg='git log --oneline --decorate --all --graph'

# Vim aliases
alias v='vim'
alias vh='vim ~'
alias g='gvim'
alias gh='gvim ~'
