# Git aliases
alias gi='git init'
alias gaa='git add .'
alias gap='git add -p'
alias gpull='git pull'
alias gp='git push'
alias gpom='git push origin master'
alias gs='git status'
alias gl='git log'
alias gma='git merge --abort'
alias gg='git log --oneline --decorate --all --graph'
alias gd='git diff'
alias grh='git reset --hard'

# HEAD^ <=> HEAD~1

# Vim aliases
alias v='vim'
alias vh='vim ~'
alias g='gvim'
alias gh='gvim ~'

# Bash aliases
alias cl='clear && clear'
alias py='python3'

# Node aliases
alias ni='node init'
alias niy='node -y init'
alias nig='node -g install'
alias nis='node --save install'
alias nisd='node --save-dev install'
alias nug='node update -g'
alias nu='node update'
alias np='node prune'
